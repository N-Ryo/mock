<% if flash %>
  <% flash.each do |message_type, message| %>
    <% p message %>
    <% p message_type %>
    <div class="alert alert-<%= message_type %>" role="alert">
      <%= message %>
      <button type="button" class="close" data-dismiss="alert" aria-label="Close">
        <span aria-hidden="true">&times;</span>
      </button>
    </div>
  <% end %>
<% end %>